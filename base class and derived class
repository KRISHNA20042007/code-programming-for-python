# Base class
class Animal:
    def make_sound(self):
        print("Some generic animal sound.")

# Derived class: Dog
class Dog(Animal):
    def make_sound(self):
        print("Woof! Woof!")

# Derived class: Cat
class Cat(Animal):
    def make_sound(self):
        print("Meow!")

# Derived class: Bird
class Bird(Animal):
    def make_sound(self):
        print("Tweet!")

# Create a list of animals (polymorphism)
animals = [Dog(), Cat(), Bird()]

# Call make_sound() on each animal
for animal in animals:
    animal.make_sound()
